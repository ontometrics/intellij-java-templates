<?xml version="1.0" encoding="UTF-8"?>
<templateSet group="Android">
  <template name="android-log-tag" value="private final static String TAG = &quot;$CLASSNAME$&quot;;" description="Create the declaration for the tag." toReformat="false" toShortenFQNames="true">
    <variable name="CLASSNAME" expression="fileNameWithoutExtension()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_DECLARATION" value="true" />
    </context>
  </template>
  <template name="android-log-statement" value="Log.$level$(TAG, &quot;$message$&quot;);" description="Send some information to the log." toReformat="false" toShortenFQNames="true">
    <variable name="level" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="message" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_DECLARATION" value="true" />
      <option name="JAVA_COMMENT" value="true" />
      <option name="JAVA_STRING" value="true" />
      <option name="COMPLETION" value="true" />
    </context>
  </template>
  <template name="android-getPreference" value="PreferenceManager.getDefaultSharedPreferences($context$).get$prefType$($preferenceName$, $defaultValue$);" description="get a preference from shared preferences" toReformat="false" toShortenFQNames="true">
    <variable name="context" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="prefType" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="preferenceName" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="defaultValue" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_DECLARATION" value="true" />
      <option name="JAVA_COMMENT" value="true" />
      <option name="JAVA_STRING" value="true" />
      <option name="COMPLETION" value="true" />
    </context>
  </template>
  <template name="android-storePreference" value="PreferenceManager.getDefaultSharedPreferences($context$).edit().put$type$($preferenceKey$,$value$).commit();" description="put a value back into shared preferences" toReformat="false" toShortenFQNames="true">
    <variable name="context" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="type" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="preferenceKey" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="value" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
      <option name="JAVA_STATEMENT" value="true" />
      <option name="JAVA_EXPRESSION" value="true" />
      <option name="JAVA_DECLARATION" value="true" />
      <option name="JAVA_COMMENT" value="true" />
      <option name="JAVA_STRING" value="true" />
      <option name="COMPLETION" value="true" />
    </context>
  </template>
</templateSet>

